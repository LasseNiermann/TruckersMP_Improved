{
  "env": {
    "browser": true,
    "jquery": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "webextensions": true
  },
  "globals": {
    "chrome": true,
    "storage": true,
    "browser": true,
    "OwnReasons": true,
    "OwnDates": true,
    "steam_data": true,
    "settings": true,
    "content_links": true,
    "construct_dates": true,
    "moment": true,
    "checkDoubleSlash": true,
    "date_buttons": true,
    "insertAtCaret": true,
    "firebase": true,
    "firebaseui": true,
    "saveSettings": true,
    "syncAllowed": true,
    "loadSettings": true,
    "loadSettingsFromFirebase": true,
    "checkBans": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "constructor-super": "warn",
    "valid-typeof": "warn"
  }
}